// Platzieren Sie Ihre Einstellungen in dieser Datei, um Standard- und Benutzereinstellungen zu Ã¼berschreiben.
{
  "autoimport.filesToScan": "src/**/*.{ts,tsx}",
  "codemetrics.basics.DecorationModeEnabled": false,
  "codemetrics.basics.MetricsForClassDeclarationsToggled": false,
  "codemetrics.nodeconfiguration.AnyKeyword": 0,
  "files.autoSave": "onFocusChange",
  "prettier.singleQuote": true,
  "prettier.trailingComma": "none",
  "search.exclude": {
    "**/node_modules": true,
    "build": true,
    "docs": true,
    "report": true,
    ".vscode": true,
    ".nyc_output": true
  },
  "typescript.referencesCodeLens.enabled": true,
  "typescript.implementationsCodeLens.enabled": true
}
