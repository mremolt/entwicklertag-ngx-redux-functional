    "@angular/router": "^4.4.0-RC.0",
    "@angularclass/hmr": "^2.1.3",
    "@dcs/ngx-utils": "^1.2.0",
    "@ngx-translate/core": "^8.0.0",
    "bootstrap": "^4.0.0-beta",
    "classlist-polyfill": "^1.2.0",
    "console-polyfill": "^0.3.0",
