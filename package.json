{
  "name": "@dcs/ngx-starter",
  "version": "1.2.0",
  "description": "Starting point for Angular projects at DCS FÃ¼rth",
  "main": "index.js",
  "scripts": {
    "clean:src": "rimraf src/**/*.ngfactory.ts src/**/*.ngsummary.json src/**/*.ngsummary.ts src/**/*.ngstyle.ts",
    "clean:artifacts": "rimraf compiled build/ngc",
    "clean:build": "rimraf build",
    "compile": "webpack --config config/webpack.prod.ts",
    "build": "run-s clean:build lint test:coverage test:coverage:check compile docs clean:artifacts",
    "build:aot": "cross-env BUILD_AOT=1 npm run build",
    "build:production": "cross-env APP_ENV=production npm run build:aot",
    "build:staging": "cross-env APP_ENV=staging npm run build:aot",
    "watch": "webpack-dev-server --config config/webpack.dev.ts",
    "watch:hmr": "npm run watch -- --inline --hot",
    "serve:production": "serve build/production -p 8080 -c-1 -s",
    "serve:staging": "serve build/staging -p 8080 -c-1 -s",
    "test": "jasmine-ts",
    "test:watch": "chokidar 'spec/**/*.ts' 'src/**/*.ts' --initial -c 'npm run test'",
    "test:coverage": "nyc  -e .ts -x \"*.spec.ts\" jasmine-ts \"spec/**/*.spec.ts\"",
    "test:coverage:check": "nyc check-coverage --statements 10 --branches 10 --functions 10 --lines 10",
    "test:e2e": "protractor protractor.conf.js",
    "lint": "tslint --project tsconfig.json --type-check",
    "docs": "typedoc --out build/docs --excludeExternals --ignoreCompilerErrors --includeDeclarations --target ES6 --exclude 'spec/**/*.ts' --mode file ./src",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "commit": "git cz",
    "commitmsg": "validate-commit-msg",
    "prepush": "npm run build:production",
    "preversion": "run-s build:production",
    "version": "run-s changelog && git add CHANGELOG.md",
    "postversion": "git push --no-verify && git push --no-verify --tags"
  },
  "keywords": [],
  "author": "Marc Remolt <marc.remolt@dcs-fuerth.de>",
  "license": "ISC",
  "devDependencies": {
    "@angular/compiler-cli": "^4.4.0-RC.0",
    "@angularclass/hmr-loader": "^3.0.4",
    "@types/jasmine": "^2.5.54",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^8.0.27",
    "@types/protractor": "^4.0.0",
    "@types/webpack": "^3.0.10",
    "@types/webpack-merge": "^0.0.5",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.1.4",
    "chokidar-cli": "^1.2.0",
    "codelyzer": "^3.1.2",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.3",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "css-to-string-loader": "^0.1.3",
    "cz-conventional-changelog": "^2.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jasmine": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "jsdom": "^11.2.0",
    "json-loader": "^0.5.7",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "ngc-webpack": "^3.2.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.2.1",
    "offline-plugin": "^4.8.3",
    "postcss-loader": "^2.0.6",
    "protractor": "^5.1.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "serve": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.6",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typedoc": "^0.8.0",
    "typescript": "^2.5.2",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.2",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "@angular-redux/router": "^6.3.1",
    "@angular-redux/store": "^6.5.7",
